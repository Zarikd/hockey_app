generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String     @id @default(uuid())
  firstName  String? @db.VarChar(255)
  secondName  String? @db.VarChar(255)
  email String  @unique @db.VarChar(255)
  password String @db.VarChar(255)
}
